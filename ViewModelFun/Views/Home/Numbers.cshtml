<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    @model int[]
    <h1>Here are numbers!</h1>
    @{
        int[] nums = {1,2,3,10,43,5};

        foreach (var num in nums)
        {
            <p>@num</p>
        }
    }    
</body>
</html>